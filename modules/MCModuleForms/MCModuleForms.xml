    <creator>Sovello Hildebrand</creator>
    <email>sovellohpmgani@gmail.com</email>
    <link>https://github.com/makhatemakhate/VMMC</link>
    <version>4.1.10.0</version>
    <path name="templates">
      <value>./templates</value>
    </path>
    <path name="classes">
