  <configurationGroup name="MCModuleForms" path="/I2CE">
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="en_US">
      <configuration name="person_can_view_child_form_prof_education" locale="en_US">
        <value>Can view prof_education child form of a person</value>
      </configuration>
      <configuration name="person_can_edit_child_form_prof_education" locale="en_US">
        <value>Can edit prof_education child forms of a person</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks_trickle_down" path="/I2CE/tasks/task_trickle_down/">
      <configuration name="person_can_view_child_form_prof_education" values="many">
        <value>person_can_view</value>
      </configuration>
      <configuration name="person_can_edit_child_form_prof_education" values="many">
        <value>person_can_view_child_form_prof_education</value>
        <value>person_can_view</value>
      </configuration>
      <configuration name="person_can_view_child_forms" values="many">
        <value>person_can_view_child_form_prof_education</value>
      </configuration>
      <configuration name="person_can_edit_child_forms" values="many">
